    {
      "cell_type": "code",
      "metadata": {
        "trusted": true,
        "id": "SZK6TFki01H3"
      },
      "source": [
        "def impute_age(cols):\n",
        "    Age = cols[0]\n",
        "    Pclass = cols[1]\n",
        "\n",
        "    if pd.isnull(Age):\n",
        "\n",
        "      if Pclass == 1:\n",
        "          return 42\n",
        "\n",
        "      elif Pclass == 2:\n",
        "          return 26\n",
        "\n",
        "      else:\n",
        "          return 23   \n",
        "\n",
        "    else:\n",
        "       return Age"
      ],
      "execution_count": 18,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "trusted": true,
        "id": "nZTaWlNB01H4"
      },
      "source": [
        "test['Age'] = test[['Age','Pclass']].apply(impute_age,axis=1)"
